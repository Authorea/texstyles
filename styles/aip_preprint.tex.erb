%\documentclass[aip,graphicx]{revtex4-1}
%\documentclass[aip,reprint]{revtex4-1}

\documentclass[%
 aip,
 jmp,%
 amsmath,amssymb,
 preprint,%
 %reprint,%
%author-year,%
%author-numerical,%
]{revtex4-1}

\usepackage{dcolumn}% Align table columns on decimal point
\usepackage{bm}% bold math
%\usepackage[mathlines]{lineno}% Enable numbering of text and display math
%\linenumbers\relax % Commence numbering lines
%\usepackage{natbib}

<%=@default_packages%>


<%=@header%>


\begin{document}

\title{<%=@title%>}

<% if !@alternative_author_string.to_s.empty? %>
 <%= @alternative_author_string %>
<% else %>
\author{<%=@first_author%>}
\affiliation{<%=@first_affiliation%>}
<% if !@coauthor_list.to_a.empty? %><% for i in (0..@coauthor_list.length-1) %>
\author{<%=@coauthor_list[i]%>}
\affiliation{<%=@coauthor_affiliations[i]%>}
<% end %>
<% end %>
<% end %>

\date{\today}% It is always \today, today,

<%= @abstract_begin_end %>


%\pacs{Valid PACS appear here}% PACS, the Physics and Astronomy
                             % Classification Scheme.
%\keywords{Suggested keywords}%Use showkeys class option if keyword
                              %display desired

